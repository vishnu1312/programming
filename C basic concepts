// string functions
int main() {
    char s1[] = "Saveetha";
    char s2[] = "Saveetha";
    char s3[] = "";
    strcpy(s3,s1);
    printf("\n%d",strcmp(s1,s2));
    printf("\n%d",strlen(s1));
    printf("\n%s",s3);
}

